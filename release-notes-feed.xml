---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/release-notes-feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml" />
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% assign releaseNotes = site.release_notes | sort: 'date' | reverse %}
    {% assign availableReleaseNotes = releaseNotes | where_exp: "note", "note.type == intro" %}
    {% for note in availableReleaseNotes limit:10 %}
      <item>
        <title>{{ note.title | xml_escape }}</title>
        <description>{{ note.content | xml_escape }}</description>
        <pubDate>{{ note.date | date_to_rfc822 }}</pubDate>
        <link>{{ note.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ note.url | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for tag in note.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in note.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
    {% endfor %}
  </channel>
</rss>
